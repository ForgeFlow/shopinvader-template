<nav>
  <ul class="pagination">
    {% if paginate.previous_page %}
      <li class="page-item">
        <a class="page-link" href="?page={{paginate.previous_page}}">
          <i class="fa fa-chevron-left"></i>
        </a>
      </li>
    {% endif %}
    {% for page_pagination in paginate.parts %}
      {%if page_pagination.hellip_break == true%}
        <li class="page-item">
          <span class="page-link">{{page_pagination.title}}</span>
        </li>
      {%else%}
      {% assign page_url = "#"  %}
        {% if page_pagination.is_link == true%}
          {% assign page_url =  page_pagination.url %}
        {% endif %}
        <li class="page-item {% if page_pagination.is_link == false%}active{%endif%}">
          <a class="page-link" href="{{page_url}}">
            {{page_pagination.title}}
          </a>
        </li>
      {%endif%}
    {%endfor%}
    {% if paginate.next %}
      <li class="page-item">
        <a class="page-link" href="?page={{paginate.next_page}}">
          <i class="fa fa-angle-right"></i>
        </a>
      </li>
    {% endif %}
  </ul>
</nav>
